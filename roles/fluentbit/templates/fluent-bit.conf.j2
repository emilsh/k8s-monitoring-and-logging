[SERVICE]
    Flush        1
    Log_Level    info
    Parsers_File parsers.conf
    Parsers_File output-elasticsearch.conf

[INPUT]
    Name         systemd
    Tag          host.*

[OUTPUT]
    Name  es
    Match host.*
    Host  {{ elasticsearch_host }}
    Port  {{ elasticsearch_port }}
    Index logs
    Type  _doc
    Logstash_Format On
